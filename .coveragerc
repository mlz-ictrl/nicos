[run]
include =
	nicos/*
	nicos_*/*
	nicos/guisupport/typedvalue.py
	nicos/clients/gui/cmdlets/*
omit =
	nicos/_vendor/*
	nicos/clients/gui/client.py
	nicos/clients/gui/config.py
	nicos/clients/gui/data.py
	nicos/clients/gui/__init__.py
	nicos/clients/gui/main.py
	nicos/clients/gui/mainwindow.py
	nicos/clients/gui/utils.py
	nicos/clients/gui/dialogs/*
	nicos/clients/gui/panels/*
	nicos/clients/gui/tools/*
	nicos/clients/gui/widgets/*
	nicos/commands/taco.py
	nicos/devices/taco/*
	nicos/devices/tango.py
	nicos/devices/tas/plotting.py
	nicos/devices/vendor/*
	nicos/guisupport/elements/*
	nicos/guisupport/tas/*
	nicos/guisupport/livewidget/*
	nicos/services/monitor/qt.py
parallel = true
